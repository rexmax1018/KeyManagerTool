{
  "CryptoSuite": {
    "KeyDirectory": "Keys",
    "AES": {
      "KeySize": 256,
      "Encoding": "UTF8"
    },
    "RSA": {
      "KeySize": 2048,
      "Encoding": "UTF8"
    },
    "ECC": {
      "Curve": "Secp256k1",
      "Encoding": "ASCII"
    },
    "UseUrlSafeBase64": true
  },
